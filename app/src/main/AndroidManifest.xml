<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.bilibili.xpref.demo"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!-- android:exported="true" for test -->
        <service android:name=".XXService" android:process=":xx" android:exported="true"/>
        <service android:name=".XXXService" android:process=":xxx" android:exported="true"/>
    </application>

</manifest>